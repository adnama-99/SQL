SELECT * FROM pokemon;

/* Order database by name */ 
SELECT * FROM pokemon ORDER BY name;

/* Only Pokemon with Grass OR Poison Type */
SELECT * FROM pokemon WHERE Type_1 = "Grass" OR  Type_2 = "Poison";

/* Only Pokemon with Grass AND Poison Type */
SELECT * FROM pokemon WHERE Type_1 = "Grass" AND  Type_2 = "Poison";

/* Classification of Base Stat Totals */
SELECT Name, CASE
    WHEN Total < 550 AND Total >= 450 THEN "High"
    WHEN Total < 450 AND Total >= 350 THEN "Medium"
    WHEN Total <350 THEN "Low"
    END "Total_Rankings"
    FROM pokemon
ORDER BY Total_Rankings;

/* Pokemon with Highest HP*/
SELECT Name, MAX(HP) AS MaxHP
FROM pokemon;
